{
  "name": "rrelayer-sdk",
  "version": "1.0.19",
  "description": "The rrelayer sdk",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --testMatch='**/__tests__/**/*.test.ts'",
    "test:relayer": "jest --testMatch='**/__tests__/relayer-client.test.ts'",
    "test:admin": "jest --testMatch='**/__tests__/admin-client.test.ts'",
    "test:client": "jest --testMatch='**/__tests__/client.test.ts'",
    "playground": "ts-node src/___PLAYGROUND___/index.ts"
  },
  "author": "joshstevens19@hotmail.co.uk",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "viem": "^2.7.19"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
